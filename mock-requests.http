    "userId": "00002",
    "hireDate": "2015-04-06T00:00:00Z",
    "terminationDate": null,
    "originalStartDate": "2015-04-06T00:00:00Z",
    "status": "REHREH"
}

### update Photo to the Employee
POST {{mockSfsfBtpEndPoint}}/Photo
Content-Type: application/json

{
    "photoType": 27,
	"userId": "test10",
	"photo": {{photoString}}
}

### Update default full Name to Employee
POST {{mockSfsfBtpEndPoint}}/User
Content-Type: application/json

{
     "userId": "test10",
    "defaultFullName":"Test9 User"
}

### update last date worked and seniorits date
PATCH {{mockSfsfBtpEndPoint}}/EmpEmployment(personIdExternal='00002',userId='00002')
Content-Type: application/json

{
    "lastDateWorked":"2016-10-17T00:00:00.000Z",
"seniorityDate":"2021-10-25T00:00:00.000Z"
}

